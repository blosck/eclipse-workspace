package myFirstProject;

public class Main {
	
	public static void main(String[] args) {
		int numEntero = 9000;
		System.out.println(numEntero);	
		
		float numFlotante = 10/3f; // Debe tener la letra "f" al final de la asignaci√≥n - permite hasta 7 decimales
		System.out.println(numFlotante);
		
		double numDoble = 10/3d; // Debe tener la letra "d" al final - permite hasta 16 decimales
		System.out.println(numDoble);
		
		String cadena = "Esta es una cadena"; // Se encierra entre comillas dobles
		System.out.println(cadena);
	}
	
	
}
