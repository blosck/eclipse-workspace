package ejemplomatrices;

public class EjemploMatrices {
	 
	public static void main(String[] args) {
		
		int edades[][];
		
		edades = new int[3][4];
		
		edades[0][0] = 10;
		edades[0][1] = 20;
		edades[0][2] = 30;
		edades[0][3] = 40;
		edades[1][0] = 50;
		edades[1][1] = 60;
		edades[1][2] = 70;
		edades[1][3] = 80;
		edades[2][0] = 90;
		edades[2][1] = 100;
		edades[2][2] = 110;
		edades[2][3] = 120;
		
		for(int i=0; i<edades.length; i++) {
			for(int j=0; j<edades[i].length; j++) {
				System.out.println("Matriz enteros Ã­ndice " + i + "-" + j + ": " + edades[i][j]);
			}
		}
	}
}
