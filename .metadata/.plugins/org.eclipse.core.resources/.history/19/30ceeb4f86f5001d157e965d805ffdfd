package sobrecargametodos;

public class sobrecargaMetodos {
	
	public static void main (String[] args) {
		System.out.println("\nResultado 1: " + Operaciones.sumar(3, 4));
		System.out.println("\nResultado 2: " + Operaciones.sumar(5, 4.1));
		System.out.println("\nResultado 3: " + Operaciones.sumar(7.1, 3));
		System.out.println("\nResultado 4: " + Operaciones.sumar(2.2, 6.8));
		
		System.out.println("\nResultado 5: " + Operaciones.sumar(3, 1L));
		System.out.println("\nResultado 6: " + Operaciones.sumar(3F, 'A'));
	}

}
